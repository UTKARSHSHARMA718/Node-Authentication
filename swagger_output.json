{
    "openapi": "3.0.0",
    "info": {
        "version": "v1.0.0",
        "title": "Swagger Demo Project",
        "description": "Implementation of Swagger with TypeScript"
    },
    "servers": [
        {
            "url": "http://localhost:5000/api/v1/",
            "description": "Auth Server"
        }
    ],
    "paths": {
        "/self": {
            "get": {
                "description": "Route to test server if working or not.",
                "responses": {
                    "200": {
                        "description": "Server is operational.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "statusCode": {
                                            "type": "integer",
                                            "example": 200
                                        },
                                        "request": {
                                            "type": "object",
                                            "properties": {
                                                "method": {
                                                    "type": "string",
                                                    "example": "GET"
                                                },
                                                "url": {
                                                    "type": "string",
                                                    "example": "/api/v1/self"
                                                }
                                            }
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "The Operation has been successful."
                                        },
                                        "data": {
                                            "type": "null",
                                            "example": null
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "statusCode": {
                                            "type": "integer",
                                            "example": 500
                                        },
                                        "request": {
                                            "type": "object",
                                            "properties": {
                                                "method": {
                                                    "type": "string",
                                                    "example": "GET"
                                                },
                                                "url": {
                                                    "type": "string",
                                                    "example": "/api/v1/self"
                                                }
                                            }
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "The Operation has been successful."
                                        },
                                        "data": {
                                            "type": "null",
                                            "example": null
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Unexpected error response."
                    }
                }
            }
        },
        "/health": {
            "get": {
                "description": "",
                "responses": {
                    "default": {
                        "description": ""
                    }
                }
            }
        },
        "/user/upload": {
            "post": {
                "description": "",
                "responses": {
                    "default": {
                        "description": "For uploading user profile images."
                    }
                }
            }
        },
        "/user/register": {
            "post": {
                "description": "",
                "responses": {
                    "default": {
                        "description": "For registring new user."
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer"
            }
        }
    }
}
